    "containerDefinitions": [
      {
        "name": "freelanceapp-container",
        "image": "149292675029.dkr.ecr.us-east-1.amazonaws.com/freelanceappapi:latest",
        "cpu": 256,
        "memory": 512,
        "portMappings": [
          {
            "containerPort": 8000,
            "protocol": "tcp"
          }
        ],
        "essential": true
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512"
  }
  